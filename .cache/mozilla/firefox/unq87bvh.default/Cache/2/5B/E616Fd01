(function() {
  $(function() {
    if ($('.accordion-box').length === 0) {
      return;
    }
    return $('.accordion-box .link-open').click(function() {
      $(this).closest('.accordion-box').find('.expanded').show();
      $(this).hide();
      return false;
    });
  });

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  this.Filter = (function() {
    function Filter(wrapperEl, paginator) {
      this.wrapperEl = wrapperEl;
      this.paginator = paginator;
      this.wrapperEl.on('click', '#all-journals', (function(_this) {
        return function(e) {
          e.preventDefault();
          $('.checkbox').attr('checked', null);
          return _this.updateJournals();
        };
      })(this));
      this.wrapperEl.on('click', '#all-journals-included', (function(_this) {
        return function(e) {
          e.preventDefault();
          $('.checkbox').attr('checked', 'checked');
          return _this.updateJournals();
        };
      })(this));
      this.wrapperEl.on('change', '.checkbox', (function(_this) {
        return function(e) {
          e.preventDefault();
          return _this.updateJournals();
        };
      })(this));
      this.wrapperEl.on('click', '#collection-select a', (function(_this) {
        return function(e) {
          e.preventDefault();
          return _this.paginator.updateCollection($(e.currentTarget).attr('href'));
        };
      })(this));
      this.wrapperEl.on('click', '#filter-select a', (function(_this) {
        return function(e) {
          var params;
          e.preventDefault();
          params = $(e.currentTarget).attr('href').split('filter=').slice(-1)[0];
          return _this.paginator.updateParams(params);
        };
      })(this));
      if ($('.checkbox').length > 0) {
        this.updateJournals(false);
      }
    }

    Filter.prototype.updateJournals = function(immediately) {
      var key, params, value, values, valuesArray;
      if (immediately == null) {
        immediately = true;
      }
      values = {
        vak: $('#vak').prop('checked'),
        scopus: $('#scopus').prop('checked'),
        repec: $('#repec').prop('checked')
      };
      valuesArray = [];
      for (key in values) {
        value = values[key];
        if (value === 'checked' || value === true) {
          valuesArray.push(key);
        }
      }
      if (valuesArray.length === 0) {
        params = valuesArray.join('_');
      } else {
        params = '';
      }
      return this.paginator.updateParams(valuesArray.join('_'), immediately);
    };

    return Filter;

  })();

}).call(this);
$(function() {
  $(".notification").mouseover(function(e) {
    $(".notification").fadeOut()
  })

  var notifyInternal = function(klass, msg, timeout) {
    if (timeout === undefined) {
      timeout = 2000
    }
    var e = $(klass)
    e.text(msg)
    e.show()

    window.setTimeout(function() {
      e.fadeOut()
    }, timeout)
  }

  window.notifyOk = function(msg) {
    notifyInternal(".notification.okay", msg, 1000)
  }
  window.notifyError = function(msg) {
    notifyInternal(".notification.error", msg, 3000)
  }
  
  gon.notifications.forEach(function(n) {
    if (n.kind === 'notice') {
      notifyOk(n.text)
    }
    if (n.kind === 'alert') {
      notifyError(n.text)
    }
  })
})
;
// Generated by CoffeeScript 1.8.0
$(function() {
  return window.handleField = function(elem, button, url, fn_params) {
    var altButtonsWrapper, buttonsWrapper, convertBack, convertForward, dummyText, elemClass, elemStyle, newElem, oldContent, placeholder, resetButton, saveButton, updateContent;
    dummyText = ['Вы можете оставить здесь свою пометку.'];
    oldContent = elem.html().replace(/<br ?\/?>/g, "\n");
    elemClass = elem.attr('class');
    elemStyle = {
      'height': elem.css('height'),
      'width': elem.css('width'),
      'font': elem.css('font'),
      'margin': elem.css('margin'),
      'padding': elem.css('padding'),
      'display': elem.css('display'),
      'line-height': elem.css('line-height'),
      'text-align': elem.css('text-align')
    };
    if ($.inArray(oldContent, dummyText) < 0) {
      placeholder = oldContent;
    } else {
      placeholder = '';
    }
    newElem = $("<textarea class=" + elemClass + ">" + placeholder + "</textarea>)").css(elemStyle);
    buttonsWrapper = button.closest('.initial-buttons');
    altButtonsWrapper = buttonsWrapper.siblings('.editing-buttons');
    saveButton = altButtonsWrapper.find('.save-link');
    resetButton = altButtonsWrapper.find('.reset-link');
    convertForward = function() {
      elem.trigger('converted-forward');
      elem.replaceWith(newElem);
      newElem.focus().autosize();
      buttonsWrapper.hide();
      return altButtonsWrapper.show();
    };
    updateContent = function(url, value, callback) {
      var params;
      params = fn_params(value);
      return $.ajax({
        type: 'PUT',
        url: url,
        data: params,
        dataType: 'json',
        success: function(ret) {
          return typeof callback === "function" ? callback(ret.text.replace(/\n/g, '<br />')) : void 0;
        }
      });
    };
    convertBack = function(ret) {
      newElem.trigger('converted-back');
      newElem.replaceWith(elem.html(ret));
      buttonsWrapper.show();
      return altButtonsWrapper.hide();
    };
    convertForward();
    saveButton.one('click', function(e) {
      var value;
      e.preventDefault();
      value = newElem.val().trim();
      if (value === '') {
        value = oldContent;
      }
      return updateContent(url, value, convertBack);
    });
    return resetButton.one('click', function(e) {
      e.preventDefault();
      return convertBack(oldContent);
    });
  };
});
// Generated by CoffeeScript 1.9.2
(function() {
  var Mediator,
    slice = [].slice;

  Mediator = (function() {
    function Mediator() {
      this.channels = {};
    }

    Mediator.prototype.subscribe = function(name, callback) {
      var base;
      (base = this.channels)[name] || (base[name] = []);
      this.channels[name].push({
        context: this,
        callback: callback
      });
      return this;
    };

    Mediator.prototype.unsubscribe = function(name, callback) {
      var i, j, len, ref, results, sub;
      ref = this.channels[name];
      results = [];
      for (i = j = 0, len = ref.length; j < len; i = ++j) {
        sub = ref[i];
        if (sub.callback === callback) {
          results.push(this.channels[name].splice(i, 1));
        } else {
          results.push(void 0);
        }
      }
      return results;
    };

    Mediator.prototype.publish = function() {
      var data, j, len, name, ref, results, sub;
      name = arguments[0], data = 2 <= arguments.length ? slice.call(arguments, 1) : [];
      if (this.channels[name]) {
        ref = this.channels[name];
        results = [];
        for (j = 0, len = ref.length; j < len; j++) {
          sub = ref[j];
          results.push(sub.callback.apply(sub.context, data));
        }
        return results;
      }
    };

    return Mediator;

  })();

  window.mediator = new Mediator();

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  this.Paginator = (function() {
    function Paginator(collectionUrl1, wrapperEl, defaultParams) {
      var base;
      this.collectionUrl = collectionUrl1;
      this.wrapperEl = wrapperEl;
      this.defaultParams = defaultParams != null ? defaultParams : '';
      this.currentPage = $('.paging-block a.active').attr('page');
      this.filterParameters = {
        filter: this.defaultParams
      };
      this.$wrapper = $(this.wrapperEl);
      this.$wrapper.on('click', '.paging-block a', (function(_this) {
        return function(e) {
          e.preventDefault();
          return _this._updatePage($(e.currentTarget).attr('page'));
        };
      })(this));
      this.$wrapper.on('load', (function(_this) {
        return function(e) {
          return $('html, body').animate({
            scrollTop: _this.wrapperEl.offset().top
          }, 80);
        };
      })(this));
      window.onpopstate = (function(_this) {
        return function(e) {
          if (e.state && _this.wrapperEl.length > 0) {
            return _this.$wrapper.html(e.state);
          }
        };
      })(this);
      if (typeof (base = window.history).replaceState === "function") {
        base.replaceState(this.$wrapper.html(), $('title'), window.location.href);
      }
    }

    Paginator.prototype.updateCollection = function(collectionUrl) {
      this.collectionUrl = collectionUrl;
      this.filterParameters.filter = '';
      return this._updatePage('');
    };

    Paginator.prototype.updateParams = function(params, immediately) {
      if (immediately == null) {
        immediately = true;
      }
      this.filterParameters.filter = params;
      if (immediately) {
        return this._updatePage(1);
      }
    };

    Paginator.prototype._updatePage = function(page) {
      this.currentPage = parseInt(page, 10) || '';
      return this._update();
    };

    Paginator.prototype._update = function() {
      var path;
      path = this.collectionUrl;
      if (this.currentPage) {
        path = path + '/' + this.currentPage;
      }
      if (this.filterParameters.filter) {
        path = path + '?' + $.param(this.filterParameters);
      }
      return $.ajax({
        url: path,
        type: 'GET',
        dataType: 'json',
        beforeSend: (function(_this) {
          return function() {
            return _this.$wrapper.addClass('disabled');
          };
        })(this),
        complete: (function(_this) {
          return function() {
            _this.$wrapper.removeClass('disabled');
            return _this.$wrapper.trigger({
              type: 'load',
              wrapper: _this.wrapperEl
            });
          };
        })(this),
        success: (function(_this) {
          return function(ret) {
            var base;
            _this.$wrapper.html(ret.html);
            return typeof (base = window.history).pushState === "function" ? base.pushState(ret.html, $('title'), path) : void 0;
          };
        })(this)
      });
    };

    return Paginator;

  })();

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  this.Tabset = (function() {
    function Tabset(wrapperEl) {
      this.wrapperEl = wrapperEl;
      $(this.wrapperEl).on('click', '.tab-list a', (function(_this) {
        return function(e) {
          e.preventDefault();
          return _this.switchTabs($(e.currentTarget));
        };
      })(this));
    }

    Tabset.prototype.switchTabs = function(thisElem) {
      var thisHolder, thisOrder;
      thisHolder = thisElem.closest('.tabset');
      thisOrder = thisElem.parent().index();
      thisHolder.children('.tab-holder').children('.tab').removeClass('active');
      thisHolder.children('.tab-holder').children('div.tab:eq(' + thisOrder + ')').addClass('active');
      thisElem.closest('ul').find('a.active').removeClass('active');
      return thisElem.addClass('active');
    };

    return Tabset;

  })();

}).call(this);
/**
 * © cyberleninka.ru
 *
 * Anton Zelenski 2012
 * zelibobla@gmail.com
 *
 * The Jquery plugin to view pictures set located in specified location in special order
 * Plugin should be attached to any DOM element with specified
 *    `id` value - no matter what this value is, just be sure it is specified
 *    `source` value -  public folder ( count from public / of site ) where full size pictures located
 *    `pages` value - pages quantity
 *
 * Pictures should be named 1.png, 2.png, 3.png and so on.
 * Plugin is awaiting thumbnails are located in `source`/thumbnails subfolder and have same names.
 *
 *
 */


(function( $ ){

    var settings = {};
    var id = null;
    var defaults = {
    'scale_min' : 50,
    'scale_max' : 200,
    'scale_step' : 50
        /* in cause of big text look for the html templates in the bottom of this code please */
    };

    var methods = {

    init : function( options ) {

      if( options ) {
        var temp = {};
        $.extend( temp, defaults );
        $.extend( temp, options );
      } else {
        temp = defaults;
      }
      id = this.attr( 'id' );
      settings[ id ] = temp;
      settings[ id ][ 'source_standart' ] = this.data( 'source' ) + '/p/';
      settings[ id ][ 'source_fullsize' ] = this.data( 'source' ) + '/f/';
      settings[ id ][ 'source_thumbnails' ] = this.data( 'source' ) + '/t/';
      settings[ id ][ 'source' ] = settings[ id ][ 'source_standart' ];
      settings[ id ][ 'pages' ] = parseInt( this.data( 'pages' ) );
      settings[ id ][ 'handler' ] = this;
      settings[ id ][ 'current_page' ] = 1;
      settings[ id ][ 'y' ] = null;
      settings[ id ][ 'scale' ] = 1;
      settings[ id ][ 'rotations' ] = {};
      settings[ id ][ 'fullsize' ] = false;
      settings[ id ][ 'default_width' ] = settings[ id ][ 'handler' ].width();
      settings[ id ][ 'default_height' ] = settings[ id ][ 'handler' ].height();
      settings[ id ][ 'img_width' ] = 0;  /* updated after load */
      settings[ id ][ 'img_height' ] = 0;  /* updated after load */
      settings[ id ][ 'similars_containers' ] = methods.retrieveSimilarsContainers();
      settings[ id ][ 'similars_container_offset' ] = 10;
      settings[ id ][ 'ctrl' ] = false;
      settings[ id ][ 'shift' ] = false;
      settings[ id ][ 'last_preview_width' ] = 100;
      settings[ id ][ 'prevent_scroll_handler' ] = false;
      settings[ id ][ 'manual_scaled' ] = false;
      settings[ id ][ 'readable_id' ] = this.data('article');
      settings[ id ][ 'bookmark' ] = this.data('bookmark');
      settings[ id ][ 'user_rate' ] = this.data('userRate');
      settings[ id ][ 'overall_value_rate' ] = this.data('overallValueRate');
      methods.build();
    },

    retrieveSimilarsContainers: function(){
      $containers = settings[ id ][ 'handler' ].find('.similars_container');
      var $copy = $containers.clone();
      $containers.remove();
      return $copy.toArray();
    },

    /**
     * Checking IE browser
     */
    browser_is_IE: function(){
      return window.navigator.userAgent.indexOf( 'MSIE ' ) > 0;
    },

    mark_earned_stars: function(){
    },

    /**
     * default windows drawing, data fill, handlers binding
     */
    build : function(){
      /**
       * build HTML grid
       */
      settings[ id ][ 'handler' ].append( settings[ id ][ 'grid' ] );
      stars_container = settings[ id ][ 'stars_container' ]( settings[ id ][ 'readable_id' ],
                                                             settings[ id ][ 'user_rate' ],
                                                             settings[ id ][ 'overall_value_rate' ] );
      for( var i = 1; i <=  settings[ id ][ 'pages' ]; i++ ){
        $( '#preview #thumbnails_container' ).append( settings[ id ][ 'thumbnail_container' ]( settings[ id ][ 'source_thumbnails' ], i ) );
        $( '#workarea' ).append( settings[ id ][ 'page_container' ]( i ) );
        if( i == settings[ id ][ 'pages' ] - 1 ){
          $( '#page_container_' + i ).append( stars_container );
        }
      }
      if( methods.browser_is_IE() ){
        $( '.doc_controls .rotate_cw, .doc_controls .rotate_ccw' ).hide();
      }

      $( '#total_pages' ).html( settings[ id ][ 'pages' ] );

      /**
       * recalc workarea and preview area size depending of window size
       */
      methods.redrawWindows();
      methods.calcDefaultScale();
      methods.mark_earned_stars();

      // $( '#resizer' ).draggable({ axis: 'x',
      //               opacity: 0.7,
      //               cursor: 'move',
      //               containment: '#viewer',
      //               stop: function( event, ui ){
      //               var new_width = ui.offset.left - $( '#viewer' ).offset().left;
      //               if( ui.offset.left - $( '#viewer' ).offset().left < 70 ){
      //                 $( '#preview' ).width( 1 ).hide().css( 'overflow', 'hidden' );
      //               } else {
      //                 $( '#preview' ).width( new_width ).show().css( 'overflow', 'auto' );
      //               }
      //               $( '#resizer' ).css( 'left', $( '#preview' ).width() );
      //               $( '#workarea' ).width( settings[ id ][ 'handler' ].width() - $( '#preview' ).width() );
      //               methods.loadPage( settings[ id ][ 'current_page' ] );
      //               }
      //             });
      $( '#resizer .handle' ).click( function(){
        if( 50 > $( '#preview' ).width() ){
          $( '#preview' ).width( settings[ id ][ 'last_preview_width' ] ).show().css( 'overflow', 'auto' );
          $( '#resizer' ).css( 'left', settings[ id ][ 'last_preview_width' ] );
          $( '#resizer .handle' ).css({ 'background-position': '17px 0px' });
        } else {
          settings[ id ][ 'last_preview_width' ] = $( '#preview' ).width();
          $( '#preview' ).width( 1 ).hide().css( 'overflow', 'hidden' ); /* width = 1 in cause of chrome bug; width 0 makes vertical scrollbar disappeared */
          $( '#resizer' ).css( 'left', 0 );
          $( '#resizer .handle' ).css({ 'background-position': '0px 0px' });
        }
          var width = ( 20 >= parseInt( $( '#preview' ).width() ) ) ? 0 : $( '#preview' ).width(); /* IE9 fix */
        $( '#workarea' ).width( settings[ id ][ 'handler' ].width() - width );
        if( 50 > $( '#preview' ).width() &&
          false == settings[ id ][ 'fullsize' ] &&
          false == settings[ id ][ 'manual_scaled' ] ){
          $( '.doc_controls .fit_hor' ).click();
        }

        methods.loadPage( settings[ id ][ 'current_page' ] );
      });


      /**
       * bind some init staff only when defaults are calculated
       */
      settings[ id ][ 'handler' ].bind( 'calc_default_scale_finished',function(){
        methods.resizePageContainers();
        methods.loadPage( settings[ id ][ 'current_page' ] );

        settings[ id ][ 'y' ] = $( '#page_container_1' ).offset().top;
      });

      /**
       * bind controls to handlers
       */
      $( '#workarea' ).scroll( methods.workareaScroll );
      $( '.thumbnail_container' ).click( function( event ){
        methods.workareaScrollTo( parseInt( $( event.target ).attr( 'page' ) ), true );
      });
      $( '#page_control' ).bind( 'keyup', function( event ){
        var index = parseInt( $( event.target ).val() );
        if( index >= 1 &&
          index <= settings[ id ][ 'pages' ] ){
            methods.workareaScrollTo( index, true );
          } else {
            var valid_index = 1;
            if( Math.abs( index - 1 ) > Math.abs( index - settings[ id ][ 'pages' ] ) ){
              valid_index = settings[ id ][ 'pages' ];
            }
            methods.workareaScrollTo( valid_index, true );
          }
      });
      $( '.page_controls .begin' ).bind( 'click', function(){
      methods.workareaScrollTo( 1, true );
      });
      $( '.page_controls .prev' ).bind( 'click', function(){
      if( settings[ id ][ 'current_page' ] > 1 ){
        methods.workareaScrollTo( settings[ id ][ 'current_page' ] - 1, true );
      }
      });
      $( '.page_controls .next' ).bind( 'click', function(){
      if( settings[ id ][ 'current_page' ] < settings[ id ][ 'pages'] ){
        methods.workareaScrollTo( settings[ id ][ 'current_page' ] + 1, true );
      }
      });
      $( '.page_controls .end' ).bind( 'click', function(){
      methods.workareaScrollTo( settings[ id ][ 'pages' ], true );
      });
      $( '.doc_controls .fit_ver' ).bind( 'click', function(){
      var new_scale = settings[ id ][ 'scale' ] * ( $( '#workarea' ).height() / $( '#page_container_1' ).height() );
      settings[ id ][ 'manual_scaled' ] = true;
      methods.resizePages( new_scale );
      });
      $( '.doc_controls .fit_hor' ).bind( 'click', function(){
      var new_scale = settings[ id ][ 'scale' ] * ( ( $( '#workarea' ).width() - 30 ) / $( '#page_container_1' ).width() );
      methods.resizePages( new_scale );
      settings[ id ][ 'manual_scaled' ] = false;
      });
      $( '.doc_controls .rotate_ccw, .doc_controls .rotate_cw' ).bind( 'click', function( event ){
      var index = settings[ id ][ 'current_page' ],
        current_rotation = 'undefined' == typeof settings[ id ][ 'rotations' ][ index ]
                 ? 0
                 : settings[ id ][ 'rotations' ][ index ],
        direction = $( event.target ).hasClass( 'rotate_cw' ) || $( event.target ).parent().hasClass( 'rotate_cw' )
              ? 'cw'
              : 'ccw';
      current_rotation += 'cw' == direction ? 90 : -90;
      current_rotation = 360 <= current_rotation
               ? 0
               : current_rotation;
      current_rotation = 0 > current_rotation
               ? 360 + current_rotation
               : current_rotation;
      current_rotation = 180 == current_rotation
               ? ( 'cw' == direction ? 270 : 90 )
               : current_rotation;
      settings[ id ][ 'rotations' ][ index ] = current_rotation;
      methods.loadPage( index );
      });

      $( '#workarea' ).dblclick( function( event ){
        $( '.fullsize_control .fullsize' ).trigger('click');
      });

      $( '.fullsize_control .fullsize' ).click( function( event ){
        if( false == settings[ id ][ 'fullsize' ] ){
          $(this).addClass( 'active' );
          settings[ id ][ 'source' ] = settings[ id ][ 'source_fullsize' ];
          document.documentElement.style.overflowY = 'hidden';
          $( '#page_' + settings[ id ][ 'current_page' ] ).remove();
          settings[ id ][ 'handler' ].css({
            'position'  : 'fixed',
            'top'   : '0px',
            'left'    : '0px',
            'width'   : '100%',
            'height'  : '100%'
          });
          methods.calcDefaultScale();
          mediator.publish('show_side_buttons');
          mediator.publish('convert_side_buttons', { applyFullSize: true });
          mediator.publish('hide_share_bar');
        } else {
          $(this).removeClass( 'active' );
          settings[ id ][ 'source' ] = settings[ id ][ 'source_standart' ];
          document.documentElement.style.overflowY = 'visible';
          settings[ id ][ 'handler' ].css({
            'position'  : 'static',
            'width'   : settings[ id ][ 'default_width' ],
            'height'  : settings[ id ][ 'default_height' ]
          });
          methods.calcDefaultScale( false );
          mediator.publish('show_share_bar');
          // we hide buttons only if special scroll value had been reached
          if ($(window).scrollTop() < $('.stats-buttons').offset().top + 100) {
            mediator.publish('hide_side_buttons', { applyFullSize: true });
          }
          mediator.publish('convert_side_buttons', { applyFullSize: false });
        }

        settings[ id ][ 'fullsize' ] = !settings[ id ][ 'fullsize' ];
        methods.redrawWindows( false );
        methods.workareaScrollTo( settings[ id ][ 'current_page' ] );

        if (settings[ id ][ 'fullsize' ]) {
          settings[ id ][ 'handler' ].addClass('is_fullsize');
        } else {
          settings[ id ][ 'handler' ].removeClass('is_fullsize');
        };
      });

      /**
      * handle keypress shortcuts
      */
      $( document ).keydown( function( event ){
        if( 17 == event.keyCode ){
          settings[ id ][ 'ctrl' ] = true;
        }
        if( 16 == event.keyCode ){
          settings[ id ][ 'shift' ] = true;
        }
      });
      $( document ).keyup( function( event ) {
      event.preventDefault();
          if( 27 == event.keyCode &&
          true == settings[ id ][ 'fullsize' ] ){
          $( '.fullsize_control .fullsize' ).click();
        }
        if( 33 == event.keyCode ) $( '.page_controls .prev' ).click();
        if( 34 == event.keyCode ) $( '.page_controls .next' ).click();

        if( 17 == event.keyCode ){
          settings[ id ][ 'ctrl' ] = false;
        }
        if( 16 == event.keyCode ){
          settings[ id ][ 'shift' ] = false;
        }
      });

      /**
      * handle window resize
      */
      var waitForFinalEvent = (function () {
          var timers = {};
          return function (callback, ms, uniqueId) {
            if (!uniqueId) {
                uniqueId = "Don't call this twice without a uniqueId";
            }
            if (timers[uniqueId]) {
                clearTimeout (timers[uniqueId]);
            }
            timers[uniqueId] = setTimeout(callback, ms);
          };
      })();

      $( window ).resize( function(){
        waitForFinalEvent( function(){
              methods.redrawWindows();
          }, 500, "some unique string");
        });
      },

      /**
      * redraw windows due to a new value of generic window or one of preview/workarea
      *
      */
      redrawWindows: function(){

        var preview_width = ( 20 >= parseInt( $( '#preview' ).width() ) ) ? 0 : $( '#preview' ).width(), /* IE9 fix */
            left_height = settings[ id ][ 'handler' ].height(),
            workarea_width = settings[ id ][ 'handler' ].width() - preview_width;

        $('#workarea')
          .height(left_height)
          .width(workarea_width);

        $('#resizer')
          .height( $( '#workarea' ).height() )
          .css({
            position: 'relative',
            top: 0,
            left: preview_width
          });

          $('#preview').height(left_height);
      },

      /**
      * load specified page and it neighbours into workarea
      * highlight current page thumbnail in preview zone
      *
      */
      loadPage: function(index, smoothscroll){

        var load = function (i){

          var img = new Image(),
              resized = false;

          img.src =  settings[ id ][ 'source' ] +
                     i +
                     '.png' +
                     ( methods.browser_is_IE() ? '?no_cache' + new Date().getTime() : '' );

          img.onload = function(){
            var width  = img.width * settings[ id ][ 'scale' ],
                height = img.height * settings[ id ][ 'scale' ]
                similars_container = settings[ id ][ 'similars_containers' ][i - 1];

            $( '#page_container_' + i ).html( '' );
            $( '#page_container_' + i ).append( settings[ id ][ 'page' ]( settings[ id ][ 'source' ], i ) );

            // we append similar articles block if exists and increase height of page wrapper
            if (settings[ id ][ 'fullsize' ] && similars_container) {
              $('#page_container_' + i).append(similars_container);
              height += $(similars_container).height() + settings[ id ][ 'similars_container_offset' ];
            }

          //if( 100 < Math.abs( $( '#page_container_' + i ).height() - height ) ) resized = true;
          $( '#page_container_' + i ).height( height );
                  $( '#page_' + i ).width( width );
          if( 'undefined' != typeof( settings[ id ][ 'rotations' ][ i ] ) ){
          if( 0 == settings[ id ][ 'rotations' ][ i ] ){
            var left = 0,
              top = 0,
              new_rotation = 'matrix(1, 0, 0, 1, 0, 0)';
          } else if( 90 == settings[ id ][ 'rotations' ][ i ] ){
            var left = ( $( '#page_' + i ).height() - $( '#page_' + i ).width() ) / 2,
              top = 0,
              new_rotation = 'matrix(0, -1, 1, 0, 0, 0)';
          } else if( 180 == settings[ id ][ 'rotations' ][ i ] ){
            var left = 0,
              top = -500,
              new_rotation = 'matrix(-1, 0, 0, -1, 0, 0)';
          } else if( 270 == settings[ id ][ 'rotations' ][ i ] ){
            var left = ( $( '#page_' + i ).height() - $( '#page_' + i ).width() ) / 2,
              top = 0,
              new_rotation = 'matrix(0, 1, -1, 0, 0, 0)';
          }

          $( '#page_' + i ).css({
            '-webkit-transform': new_rotation,
            '-moz-transform': new_rotation,
            '-o-transform': new_rotation,
            '-ms-transform': new_rotation,
            'left': left,
            'top': top
          });

          $( '#page_container_' + i ).css({ 'top'   : -top });
          }
          //callback( resized );
              }
          }

      /*
      var resized_just_one = false,
        finished_to_load = 0,
        should_to_load = 0;
      */
        $.each( $( '.page_container' ), function( i, container ){
          if( 2 > Math.abs( index - ( i + 1 ) ) ){
            //should_to_load++;
            load( ( i + 1 ), function( resized ){
              //finished_to_load++;
              //if( resized ) resized_just_one = true;
            });
          } else {
            $( container ).html( '' );
          }
        });
      /*
      var reposition = setInterval( function(){
        //console.log( 'should_to_load:' + should_to_load + '; finished:' + finished_to_load );
        if( finished_to_load < should_to_load ) return;
        clearInterval( reposition );
        if( resized_just_one ){
          //console.log( 'resized!!! restart workareaScrollTo( index:' + index + ', smooth:' + smoothscroll + ' )' );
          console.log( resized_callback );
          if( resized_just_one &&
            'function' == typeof resized_callback ) resized_callback();
        }
      }, 300 );
      */
          $( '.thumbnail_container' ).removeClass( 'active' );
          $( '#thumbnail_container_' + index ).addClass( 'active' );
          methods.previewScrollTo( index, smoothscroll );

      // make request via AJAX to store user's reading progress, multiple requests prevented by this code
          if( gon.current_user_email !== false ){
            var needToSave = settings[ id ][ 'bookmark' ] < settings[ id ][ 'current_page' ];
            if( $( '#page_control' ).val() != index && needToSave ){

              settings[ id ][ 'bookmark' ] = settings[ id ][ 'current_page' ];
            }
          }
          $( '#page_control' ).val( index );
      },

      /**
      * handle workarea scrolling
      *
      */
      workareaScroll: function(){
      // prevent recursion
      if( settings[ id ][ 'prevent_scroll_handler' ] ) return;

      var new_y = $( '#page_container_1' ).offset().top,
          diff = new_y - settings[ id ][ 'y' ],
          current_page = parseInt( settings[ id ][ 'current_page' ] ),
          next_page = current_page + 1,
          prev_page = current_page - 1;
      /* move down */
      if( diff < 0 && current_page < settings[ id ][ 'pages' ] ){

            /* slow scrolling */
            if( true == methods.isScrolledIntoWorkarea( $( '#page_container_' + next_page ) ) &&
                Math.abs( diff ) <= 200 ){

                  settings[ id ][ 'current_page' ]++;
            methods.loadPage( settings[ id ][ 'current_page' ], true );

      /* fast scrolling */
      } else if( false == methods.isScrolledIntoWorkarea( $( '#page_container_' + next_page ) ) &&
              Math.abs( diff ) > 200 ){
                settings[ id ][ 'current_page' ] = methods.definePageByY( new_y );
        methods.loadPage( settings[ id ][ 'current_page' ] );
      }
      /* move up */
          } else if( new_y > settings[ id ][ 'y' ]  &&
                   current_page > 1 ){

      /* slow scrolling */
      if( true == methods.isScrolledIntoWorkarea( $( '#page_container_' + prev_page ) ) &&
        Math.abs( diff ) <= 200 ){

        settings[ id ][ 'current_page' ]--;
        methods.loadPage( settings[ id ][ 'current_page' ], true );

      /* fast scrolling */
      } else if ( false == methods.isScrolledIntoWorkarea( $( '#page_container_' + prev_page ) ) &&
            Math.abs( diff ) > 200 ){
                settings[ id ][ 'current_page' ] = methods.definePageByY( new_y );
        methods.loadPage( settings[ id ][ 'current_page' ] );
      }
          }
      /*
          console.log( 'current_page:' + current_page,
             'next_page:' + next_page,
             'prev_page:' + prev_page,
             'new_y:' + new_y,
                       'settings[ id ][ y ]:' + settings[ id ][ 'y' ],
             'Math.abs( diff ):' + Math.abs( diff ),
                       'isScrolledIntoWorkarea( page_container_' + next_page + ' ):' +
                       methods.isScrolledIntoWorkarea( $( '#page_container_' + next_page ) ) );
          console.log( 'current_page:' + settings[ id ][ 'current_page' ] );*/
          settings[ id ][ 'y' ] = new_y;
      },


      /**
      * scroll workarea to specified page
      *
      */
      workareaScrollTo: function( i, smooth ){

            var target_y = $( '#page_container_' + i ).offset().top -
                     $( '#page_container_1' ).offset().top;
      if( true == smooth ){
              $( '#workarea' ).stop().animate(
          { 'scrollTop' : target_y },
          1000,
          null,
          function(){
//            settings[ id ][ 'current_page' ] = i;
            var target_y = $( '#page_container_' + i ).offset().top -
                     $( '#page_container_1' ).offset().top;
            $( '#workarea' ).stop().animate({ 'scrollTop' : target_y }, 600 );
            methods.loadPage( i, smooth );
          }
        );
      } else {
        settings[ id ][ 'prevent_scroll_handler' ] = true;
        setTimeout( function(){ settings[ id ][ 'prevent_scroll_handler' ] = false; }, 1000 );
        $( '#workarea' ).scrollTop( target_y );
//        settings[ id ][ 'current_page' ] = i;
        methods.loadPage( i, smooth );
      }
      },

      /**
      * scroll preview area to specified page
      *
      */
      previewScrollTo: function( i, smooth ){
      var target_y = $( '#thumbnail_container_' + i ).offset().top -
               $( '#thumbnail_container_1' ).offset().top;
      if( true == smooth ){
        $( '#preview' ).animate({ 'scrollTop' : target_y }, 500 );
      } else {
        $( '#preview' ).scrollTop( target_y );
      }
      },

      /**
      * calculate default scale value
      *
      */
      calcDefaultScale: function( mean_preview ){
          var img = new Image();
          img.src =  settings[ id ][ 'source' ] + '1.png' +
           ( methods.browser_is_IE()
             ? '?no_cache' + new Date().getTime() /* always new hash due to IE9 bug */
             : '' );

          img.onload = function(){ /* IE9 if no hash upper, this event wouldn't fire */
      settings[ id ][ 'img_width' ] = img.width;
      settings[ id ][ 'img_height' ] = img.height;
      if( true == settings[ id ][ 'fullsize' ] ) {
        settings[ id ][ 'scale' ] = 1
      } else {
        if( 'undefined' == typeof mean_preview ||
          true == mean_preview ){
          settings[ id ][ 'scale' ] = ( $( '#workarea' ).width() - 30 ) / img.width;
        } else {
          settings[ id ][ 'scale' ] = 1;// ( settings[ id ][ 'handler' ].width() - 30 ) / img.width
        }

      }
      settings[ id ][ 'handler' ].trigger( 'calc_default_scale_finished' );
          }
      },

      /**
      * recalc page_containers size based on page_1.png size
      *
      */
      resizePageContainers: function(){

        var page_container_height = settings[ id ][ 'img_height' ] * settings[ id ][ 'scale' ],
            page_container_width  = settings[ id ][ 'img_width' ]  * settings[ id ][ 'scale' ];

        $('.page_container, .page_container img').css({
          width: page_container_width,
          height: page_container_height
        });
      },

      /**
      * check if specified element is become viewable while scrolling
      *
      */
      isScrolledIntoWorkarea: function( elem ){
            var docViewTop = $( window ).scrollTop(),
              docViewBottom = docViewTop + $( window ).height(),
              delta = $( window ).height() / 3;
            if( null == $( elem ).offset() ) return false;
            var elemTop = $( elem ).offset().top,
              elemBottom = elemTop + $( elem ).height();
            //console.log( "elem:" + elemTop + ", " + elemBottom + ";  workarea:" + docViewTop + ", " + docViewBottom )
            return ( elemTop < ( docViewBottom - delta ) ) &&
                   ( elemBottom > ( docViewTop + delta ) );
      },

      /**
      * define what page is in viewport by specified y value
      *
      */
      definePageByY: function( y ){
            var page_height = $( '#page_container_1' ).height();
      return Math.floor( Math.abs( ( y - 100 ) / page_height ) ) + 1;
      },

    /**
    * set up new specified scale value and resize all pages
    *
    */
    resizePages: function( new_scale ){
      settings[ id ][ 'scale' ] = new_scale;
      methods.resizePageContainers();
      methods.workareaScrollTo( settings[ id ][ 'current_page' ] );
    },

    };

    /**
    * attach plugin to JQuery stack
    *
    */
  $.fn.viewer = function( method ) {

      // Method calling logic
      if ( methods[ method ] ) {
        return methods[ method ].apply( this, Array.prototype.slice.call( arguments, 1 ));
      } else if ( typeof method === 'object' || !method ) {
        return methods.init.apply( this, arguments );
      } else {
        $.error( 'Method ' +  method + ' does not exist on jQuery.viewer' );
      }

  };

    /**
    * in cause of big text html templates are placed here
    * You can change these templates, but make sure you're left the same element's `id` and `class` values
    * (it being used later after templates attachement)
    *
    */
    defaults[ 'grid' ] = '\
      <div class="clear"></div>\
      <div id="preview">\
         <div id="thumbnails_container"></div>\
      </div>\
      <div class="wrapper_main">\
        <div id="controls">\
          <div class="fullsize_control">\
            <button class="fullsize" title="Переключение полноэкранного режима">\
              <img class="fson" src="/assets/viewer/zoom_in.png" />\
              <img class="fsoff" src="/assets/viewer/zoom_out.png" />\
            </button>\
          </div>\
          <div class="page_controls">\
            <div class="current">\
              <button class="begin" title="Перейти на первую страницу">\
                <img src="/assets/viewer/first.png" />\
              </button>\
              <button class="prev" title="Перейти на предыдущую страницу (PgUp)">\
                <img src="/assets/viewer/prev.png" />\
              </button>\
            </div>\
            <div class="pages-numbers">\
              <input type="text" id="page_control" value="1"/>\
              /\
              <span id="total_pages"></span>\
            </div>\
            <div class="current">\
              <button class="next" title="Перейти на следующую страницу (PgDown)">\
                <img src="/assets/viewer/next.png" />\
              </button>\
              <button class="end" title="Перейти на последнюю страницу">\
                <img src="/assets/viewer/last.png" />\
              </button>\
            </div>\
          </div>\
          <div class="doc_controls">\
            <div class="rotate">\
              <button class="fit_hor" title="Установить масштаб «По ширине страницы»">\
                <img src="/assets/viewer/width.png" />\
              </button>\
              <button class="fit_ver" title="Установить масштаб «По высоте страницы»">\
                <img src="/assets/viewer/height.png" />\
              </button>\
              <button class="rotate_cw" title="Повернуть страницу по часовой стрелке">\
                <img src="/assets/viewer/rotate_left.png" />\
              </button>\
              <button class="rotate_ccw" title="Повернуть страницу против часовой стрелки">\
                <img src="/assets/viewer/rotate_right.png" />\
              </button>\
            </div>\
          </div>\
        </div>\
        <div id="workarea"></div>\
      </div>\
      <div id="resizer" title="Изменить размер">\
        <div class="handle"></div>\
      </div>\
      <div id="viewer_end"></div>';

    defaults[ 'thumbnail_container' ] = function( source, index ){
        return "<div class='thumbnail_container' id='thumbnail_container_" + index + "'>\
                    <img src='" + source + index + ".png' page='" + index + "' />\
                    <div>" + index + "</div>\
                </div>";
    }

    defaults[ 'page' ] = function( source, index ){
        return '<img src="' + source + index + '.png" id="page_' + index + '" />';
    }

    defaults[ 'page_container' ] = function( index ){
        return "<div class='page_container' id='page_container_" + index + "' page='" + index + "'></div>";
    }

    defaults[ 'stars_container' ] = function( readable_id, user_rate, overall_value_rate ){
      var rate_phrase = user_rate == '' || user_rate == undefined ? "Оцените статью " : "Моя оценка &mdash; " + user_rate;
      var overall_value_rate = overall_value_rate > 0 ? overall_value_rate : ''
      return '\
        <div class = "rate-article-comment-title">\
          <span class = "user-rate">\
            ' + rate_phrase + '\
          </span>\
        </div>\
        <div class = "rate-article-comment" data-rateable-id = "' + readable_id + '" >\
          <div class="stars need-signin">\
            <span class="star"></span>\
            <span class="star"></span>\
            <span class="star"></span>\
            <span class="star"></span>\
            <span class="star"></span>\
          </div>\
          <span class="rates-value">\
            ' + overall_value_rate + '\
          </span>\
        </div>\
        ';
    }

})( jQuery );
// Generated by CoffeeScript 1.9.2
(function() {
  $(function() {
    var loadWidget, widgetWrapper;
    if ($('#widget').length === 0) {
      return;
    }
    widgetWrapper = $('#widget');
    loadWidget = function(elem, term) {
      if (term == null) {
        term = false;
      }
      return $.ajax({
        url: '/widgets',
        type: 'GET',
        data: {
          term: term
        },
        dataType: 'json',
        success: function(data) {
          var tabset;
          elem.html(data.html);
          elem.fadeIn(100);
          return tabset = new Tabset(elem);
        }
      });
    };
    widgetWrapper.on('click', '.next', function(e) {
      var $container, $list, $newList;
      e.preventDefault();
      $container = $(this).closest('.gallery-info').find('.gallery');
      $list = $container.find('ul');
      if ($list.is(':animated')) {
        return false;
      }
      if (parseInt($list.css('margin-top')) === -($list.height() - 180)) {
        $newList = $list.clone().css('margin-top', 0);
        $container.append($newList);
        return $list.animate({
          'margin-top': '-=180px'
        }, 300, function() {
          return $list.remove();
        });
      } else {
        return $list.animate({
          'margin-top': '-=180px'
        }, 300);
      }
    });
    widgetWrapper.on('click', '.prev', function(e) {
      var $container, $list, $newList;
      e.preventDefault();
      $container = $(this).closest('.gallery-info').find('.gallery');
      $list = $container.find('ul');
      if ($list.is(':animated')) {
        return false;
      }
      if (parseInt($list.css('margin-top')) === 0) {
        $newList = $list.clone().css('margin-top', -$list.height());
        $container.prepend($newList);
        return $newList.animate({
          'margin-top': '+=180px'
        }, 300, function() {
          return $list.remove();
        });
      } else {
        return $list.animate({
          'margin-top': '+=180px'
        }, 300);
      }
    });
    return loadWidget(widgetWrapper, gon.term);
  });

}).call(this);
(function() {
  $(function() {
    var AjaxTabs;
    if ($('#ajax-tabs').length === 0) {
      return;
    }
    AjaxTabs = (function() {
      function AjaxTabs(elemRef) {
        this.$elem = $(elemRef);
        this.$elem.on('click', '.link-tab, .link-page', (function(_this) {
          return function(e) {
            e.preventDefault();
            if (!_this.disabled) {
              return _this.loadContent($(e.currentTarget).attr('href'));
            }
          };
        })(this));
        window.onpopstate = (function(_this) {
          return function(e) {
            if (e.state && _this.$elem.length > 0) {
              return _this.$elem.html(e.state);
            }
          };
        })(this);
      }

      AjaxTabs.prototype.disable = function() {
        this.$elem.addClass('disabled');
        return this.disabled = true;
      };

      AjaxTabs.prototype.enable = function() {
        this.$elem.trigger('load');
        this.$elem.removeClass('disabled');
        return this.disabled = false;
      };

      AjaxTabs.prototype.setHistory = function(content, url) {
        var base;
        return typeof (base = window.history).pushState === "function" ? base.pushState(content, $('title'), url) : void 0;
      };

      AjaxTabs.prototype.loadContent = function(contentUrl) {
        return $.ajax({
          method: 'GET',
          dataType: 'json',
          url: contentUrl,
          beforeSend: (function(_this) {
            return function() {
              return _this.disable();
            };
          })(this),
          complete: (function(_this) {
            return function() {
              return _this.enable();
            };
          })(this),
          success: (function(_this) {
            return function(ret) {
              _this.setHistory(ret.content, contentUrl);
              return _this.appendContent(ret.content);
            };
          })(this)
        });
      };

      AjaxTabs.prototype.appendContent = function(content) {
        return this.$elem.html(content);
      };

      return AjaxTabs;

    })();
    return new AjaxTabs('#ajax-tabs');
  });

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  $(function() {
    var $body, $sideButtons, $slidingBox, buttonsHandle, convertButtons, hideButtons, increaseDownloadedCounter, selectQuote, showBox, showButtons, slidingBoxHandle, tabset;
    if ($('#viewer').length === 0) {
      return;
    }
    tabset = new Tabset($('.popup'));
    $slidingBox = $('.combo-box');
    $sideButtons = $('#side-buttons');
    $body = $('html,body');
    slidingBoxHandle = $('#sliding-box-top').offset().top;
    buttonsHandle = $('.stats-buttons').offset().top + 100;
    showBox = function() {
      var boxWidth;
      if ($(window).width() > 1000) {
        boxWidth = $('#viewer').offset().left - 28;
        if ($(window).scrollTop() > slidingBoxHandle) {
          $slidingBox.css({
            width: boxWidth
          });
          return $slidingBox.animate({
            left: 0
          }, 300);
        } else {
          return $slidingBox.stop(true).animate({
            left: '-1000px'
          }, 200);
        }
      }
    };
    if ($(window).width() < 1200) {
      $slidingBox.find('li').last().remove();
    }
    showButtons = function(options) {
      return $sideButtons.fadeIn(300);
    };
    hideButtons = function(options) {
      return $sideButtons.fadeOut(300);
    };
    convertButtons = function(options) {
      if (options && options.applyFullSize) {
        return $sideButtons.addClass('fullsize');
      } else {
        return $sideButtons.queue(function() {
          $sideButtons.removeClass('fullsize');
          return $sideButtons.dequeue();
        });
      }
    };
    mediator.subscribe('show_side_buttons', showButtons);
    mediator.subscribe('hide_side_buttons', hideButtons);
    mediator.subscribe('convert_side_buttons', convertButtons);
    mediator.subscribe('show_similars_box', showBox);
    $(window).scroll(function() {
      if ($(window).scrollTop() > buttonsHandle) {
        mediator.publish('show_side_buttons');
      } else {
        mediator.publish('hide_side_buttons');
      }
      return mediator.publish('show_similars_box');
    });
    $slidingBox.on('click', '.link-more', function(e) {
      var similarsOffset;
      e.preventDefault();
      similarsOffset = $('.similar-articles').offset().top;
      return $body.animate({
        scrollTop: similarsOffset
      }, 600, function() {
        return $slidingBox.stop(true).fadeOut(300);
      });
    });
    $slidingBox.on('click', '.link-close', function(e) {
      e.preventDefault();
      return $slidingBox.stop(true).fadeOut(300);
    });
    selectQuote = function(elem) {
      var range, selection;
      if (document.body.createTextRange) {
        range = document.body.createTextRange();
        range.moveToElementText(elem[0]);
        return range.select();
      } else if (window.getSelection) {
        selection = window.getSelection();
        range = document.createRange();
        range.selectNode(elem[0]);
        selection.removeAllRanges();
        return selection.addRange(range);
      }
    };
    $('.tabset-inner-hold').on('click', '.tab', function(e) {
      return selectQuote($(this));
    });
    increaseDownloadedCounter = function() {
      var countElem, countVal;
      countElem = $('.downloads-count');
      countVal = parseInt(countElem.text());
      return countElem.text(++countVal);
    };
    $('.pdf-btn').on('click', increaseDownloadedCounter);
    $('#viewer').viewer();
    $('.start-reading').on('click', function(e) {
      return $('.fullsize_control .fullsize').click();
    });
    $('.link-comment').on('click', function(e) {
      var $target;
      $target = $($(this).attr('href'));
      return $body.animate({
        scrollTop: $target.offset().top - 20
      }, 700);
    });
    $('.date-block').on('click', '.num', function(e) {
      return window.location = $(this).data('link') + "#issues-list-title";
    });
    $('#side-buttons').on('mouseenter', 'div', function(e) {
      $('.tooltip').hide();
      return $(this).find('.tooltip').fadeIn(200);
    });
    $('#side-buttons').on('mouseout', 'div', function(e) {
      $('.tooltip').hide();
      return $(this).find('.tooltip').fadeOut(200);
    });
    return $('.link-author').on('click', function(e) {
      var $link;
      e.preventDefault();
      e.stopPropagation();
      $link = $(this);
      return $.ajax({
        url: $link.attr('href'),
        method: 'PUT',
        dataType: 'JSON',
        complete: function(ret) {
          var data;
          data = JSON.parse(ret.responseText);
          if (ret.status === 302) {
            return window.location = data.path;
          } else if (ret.status === 200) {
            if (data.valid) {
              notifyOk('Заявка отправлена');
              return $('.link-author').addClass('is_touched').find('.btn-text').text('проверяется');
            } else {
              return notifyError('Запрос не может быть отправлен: ' + data.errors);
            }
          }
        }
      });
    });
  });

}).call(this);
