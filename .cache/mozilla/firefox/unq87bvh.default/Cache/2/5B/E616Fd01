(function() {
  $(function() {
    var $content, highlight, initDatepicker, initGooglePlacesAutocomplete, initImageInput, initLanguagesInput, initShowForm, initSlugInput, initValuesLanguagesInput;
    if ($('.tabset-profile').length === 0) {
      return;
    }
    $content = $('.tabset-profile');
    initDatepicker = function() {
      var $birthdate;
      $birthdate = $('.birthdate');
      $birthdate.datepicker({
        format: 'dd.mm.yyyy',
        language: 'ru',
        changeYear: true,
        changeMonth: true,
        weekStart: 1
      });
      return $('.birthdate-image').on('click', function(e) {
        e.preventDefault();
        return $birthdate.datepicker('show');
      });
    };
    initShowForm = function() {
      return $('.show-form').on('click', function(e) {
        e.preventDefault();
        $(this).parent().next().slideToggle(200);
        return $(this).toggleClass('title-expanded');
      });
    };
    initImageInput = function() {
      return $('.image-field').on('change', function(e) {
        var $form;
        $form = $(this).closest('form');
        $form.find('img').addClass('disabled');
        $.ajax({
          headers: { Accept : "application/json; charset=utf-8" },
          url: $form.attr('action'),
          type: 'POST',
          data: new FormData($form[0]),
          processData: false,
          contentType: false
        }).success(function(ret) {
            var img;
            if (ret.success) {
              notifyOk(ret.text)
            } else {
              notifyError(ret.text)
            }
            img = new Image();
            img.onload = function(e) {
              return $('.img-mask').html(img);
            };
            img.width = 100;
            return img.src = ret.avatar;
        })
      });
    };
    initSlugInput = function() {
      var $slugInput, $spanPreSlug;
      $spanPreSlug = $('span.pre_slug');
      $slugInput = $spanPreSlug.parents().first().children('input').first();
      if (!$spanPreSlug.hasClass('disabled')) {
        $slugInput.click(function() {
          return $('#slug_info').show();
        });
        $slugInput.focusout(function() {
          $('#slug_info').hide();
        });
      }
    };
    initLanguagesInput = function() {
      var $language_input, $multiple_select, languages_codes;
      $multiple_select = $('select[multiple=multiple]#languages');
      $language_input = $('#user_languages');
      if ($language_input.val() !== void 0) {
        languages_codes = $language_input.val().split(',');
        if (languages_codes[0] !== '') {
          $(languages_codes).each(function() {
            return $multiple_select.find("option[value=" + this + "]").prop('selected', true);
          });
        }
      }
      $multiple_select.selectize();
      return $multiple_select.on('change', function(e) {
        var arr, in_arr, ru_code;
        arr = $(this).val();
        ru_code = 'ru';
        in_arr = $.inArray(ru_code, arr);
        if (in_arr !== -1) {
          arr.splice(in_arr, ru_code);
          arr.unshift(ru_code);
        }
        $language_input.val($(this).val());
        return false;
      });
    };
    initValuesLanguagesInput = function() {
      var $language_input, languages_codes;
      $language_input = $('#user_languages');
      languages_codes = $language_input.val().split(',');
      return $(languages_codes).each(function() {
        return $multiple_select.find("option[value=" + this + "]").prop('selected', true);
      });
    };
    initGooglePlacesAutocomplete = function() {
      var $locationId, $locationLat, $locationLng, $locationName, autocomplete, options;
      if ($('#user_location_name').length === 0) {
        return;
      }
      $locationName = $('#user_location_name');
      $locationId = $('#user_location_id');
      $locationLat = $('#user_location_lat');
      $locationLng = $('#user_location_lng');
      options = {
        types: ['(cities)']
      };
      autocomplete = new google.maps.places.Autocomplete($locationName[0], options);
      $locationName.one('mouseover', function() {
        if ($('.pac-container').length > 0) {
          return $('.pac-container').addClass('profile');
        }
      });
      return google.maps.event.addListener(autocomplete, 'place_changed', function() {
        var id, lat, lng, place, ref;
        place = autocomplete.getPlace();
        if (place.geometry.location) {
          ref = [place.place_id, place.geometry.location.k, place.geometry.location.D], id = ref[0], lat = ref[1], lng = ref[2];
          $locationId.val(id);
          $locationLat.val(lat);
          return $locationLng.val(lng);
        }
      });
    };
    initDatepicker();
    initShowForm();
    initImageInput();
    initSlugInput();
    initLanguagesInput();
    initGooglePlacesAutocomplete();
    $content.on('load', initDatepicker);
    $content.on('load', initShowForm);
    $content.on('load', initImageInput);
    $content.on('load', initSlugInput);
    $content.on('load', initLanguagesInput);
    $content.on('load', initGooglePlacesAutocomplete);
    highlight = function($input) {
      if ($input.val() === '') {
        $input.parents('div').first().addClass('text-input-empty');
        $input.change(function() {
          $input.parents('div').first().removeClass('text-input-empty');
        });
      }
    };
    if (window.location.hash === '#not_all_attributes') {
      notifyOk("Вам следует заполнить поля 'Фамилия', 'Имя', 'Отчество', 'Email'");
      highlight($('#user_first_name'));
      highlight($('#user_middle_name'));
      highlight($('#user_last_name'));
      highlight($('#user_email'));
    }
    return $('.access_check_box').change(function() {
      var $checkbox, access;
      $checkbox = $(this);
      if ($checkbox.prop('checked') === true) {
        access = 'show';
      } else {
        access = 'not_show';
      }
      return $.ajax({
        data: {
          authentication: {
            access: access
          }
        },
        url: "/users/authentication_accesses/" + ($checkbox.attr('id')),
        method: 'PUT',
        success: function() {
          return notifyOk('Отображение изменено');
        },
        error: function() {
          return notifyError('Ошибка! Отображение не изменено');
        }
      });
    });
  });

}).call(this);
