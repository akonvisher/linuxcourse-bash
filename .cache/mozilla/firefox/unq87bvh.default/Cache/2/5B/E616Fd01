// Generated by CoffeeScript 1.8.0
$(function() {
  var createComment, deleteComment, stashComment, voteForComment;
  if ($('.comment-votes-link').length > 0 && $('#new_comment').length === 0) {
    $('.comment-votes-link').click(function(e) {
      return e.preventDefault();
    });
  }
  if ($('#comments-list').length === 0) {
    return;
  }
  voteForComment = function(id, value, callback) {
    if (value === 1 || value === -1) {
      return $.ajax({
        type: 'POST',
        url: "/comments/" + id + "/vote",
        data: {
          value: value
        },
        dataType: 'json',
        success: function(ret) {
          return ((ret != null ? ret.success : void 0) != null) && (typeof callback === "function" ? callback(ret) : void 0);
        }
      });
    }
  };
  $('#comments-list').on('click', '.vote-link', function(e) {
    var callback, commentId, value;
    e.preventDefault();
    commentId = $(this).data('id');
    if ($(this).hasClass('minus')) {
      value = -1;
    } else if ($(this).hasClass('plus')) {
      value = 1;
    }
    callback = (function(_this) {
      return function(ret) {
        var minusText, parent, plusText;
        parent = $(_this).parent();
        plusText = ret.votes_up_count !== 0 ? "+" + ret.votes_up_count : ret.votes_up_count;
        minusText = ret.votes_down_count !== 0 ? "-" + ret.votes_down_count : ret.votes_down_count;
        parent.find('.plus').text(plusText);
        return parent.find('.minus').text(minusText);
      };
    })(this);
    return voteForComment(commentId, value, callback);
  });
  $('#comments-list').on('click', '.edit-link', function(e) {
    var button, elem, id;
    e.preventDefault();
    button = $(this);
    elem = button.closest('.text-holder').find('.comment-body');
    id = elem.data('id');
    return window.handleField(elem, button, "/comments/" + id + "/", function(v) {
      return {
        value: v
      }
    });
  });
  createComment = function(comment) {
    return $.ajax({
      type: 'POST',
      url: '/comments',
      data: {
        comment: comment
      },
      dataType: 'json',
      error: function(ret) {
        if (ret.status === 401) {
          stashComment(comment);
          return popup.showPopup('signin');
        }
      },
      beforeSend: function() {
        return $('#comments-list').animate({
          'opacity': '0.3'
        }, 500);
      },
      complete: function() {
        return $('#comments-list').animate({
          'opacity': '1'
        }, 500);
      },
      success: function(ret) {
        return $('#comments-list').html(ret.html);
      }
    });
  };
  $('.comment-form').on('submit', '.new_comment', function(e) {
    var comment;
    e.preventDefault();
    comment = {};
    comment.article_id = $(this).find('#comment_article_id').val();
    comment.answer_to = $(this).find('#comment_answer_to').val();
    comment.text = $(this).find('#comment_text').val();
    if (comment.text.length !== 0) {
      createComment(comment);
      return $(this).find('.new-comment').val('');
    }
  });
  deleteComment = function(url) {
    return $.ajax({
      type: 'POST',
      url: url,
      data: {
        _method: 'DELETE'
      },
      dataType: 'json',
      beforeSend: function() {
        return $('#comments-list').animate({
          'opacity': '0.3'
        }, 500);
      },
      complete: function() {
        return $('#comments-list').animate({
          'opacity': '1'
        }, 500);
      },
      success: function(ret) {
        return $('#comments-list').html(ret.html);
      }
    });
  };
  $('#comments-list').on('click', '.delete-link', function(e) {
    e.preventDefault();
    return deleteComment($(this).attr('href'));
  });
  stashComment = function(comment) {
    $.cookie('auto_comment_article_id', comment.article_id, {
      path: '/'
    });
    $.cookie('auto_comment_answer_to', comment.answer_to, {
      path: '/'
    });
    return $.cookie('auto_comment_text', comment.text, {
      path: '/'
    });
  };
  return $('.new-comment').autosize();
});
// Generated by CoffeeScript 1.9.2
(function() {
  $(function() {
    var convertFavButton, createFavorite, stashFavorite;
    convertFavButton = function(elem, isFavorite, count) {
      if (isFavorite) {
        elem.addClass('is-favorite');
        elem.find('.btn-text').text('в избранном');
        elem.closest('.general-block').removeClass('removed');
      } else {
        elem.removeClass('is-favorite');
        elem.find('.btn-text').text('в избранное');
        elem.closest('.general-block').addClass('removed');
      }
      elem.data('favorite', isFavorite);
      return $('.favorites-count').text(count);
    };
    createFavorite = function(e) {
      var params, url;
      e.preventDefault();
      url = '/readings';
      params = {
        reading: {
          readable_type: $(this).data('readable-type'),
          readable_id: $(this).data('readable-id'),
          favorite: !$(this).data('favorite')
        }
      };
      return $.ajax({
        type: 'PUT',
        url: url,
        data: params,
        dataType: 'json',
        success: (function(_this) {
          return function(ret) {
            var elem;
            elem = $(_this);
            if ($('.fav-btn.linked').length > 1) {
              elem = $('.fav-btn.linked');
            }
            return convertFavButton(elem, ret.reading.favorite, ret.favorites_count);
          };
        })(this)
      });
    };
    stashFavorite = function(data) {
      $.cookie('auto_favorite_readable_type', data.readableType, {
        path: '/'
      });
      return $.cookie('auto_favorite_readable_id', data.readableId, {
        path: '/'
      });
    };
    return $('body').on('click', '.fav-btn', createFavorite);
  });

}).call(this);
