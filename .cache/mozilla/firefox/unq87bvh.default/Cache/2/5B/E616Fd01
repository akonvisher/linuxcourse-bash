(function() {
  $(function() {
    var $autofocusField;
    if ($('input').length === 0) {
      return;
    }
    $('input, textarea').each(function() {
      return $(this).placeholder();
    });
    $('form-search').on('submit', function(e) {
      if ($(this).find('.text-input').val() === '') {
        return e.preventDefault();
      }
    });
    $('#wrapper').on('focus', '.input-wrapper input, .input-wrapper textarea', function() {
      var $elem, $wrapper;
      $elem = $(this);
      $wrapper = $elem.closest('.input-wrapper');
      $elem.addClass('focus text-active');
      $wrapper.addClass('parent-focus parent-active');
      return $elem.one('blur', function() {
        if ($elem.data('prevent-blur') && $elem.val() !== '') {

        } else {
          $elem.removeClass('focus text-active');
          return $wrapper.removeClass('parent-focus parent-active');
        }
      });
    });
    $autofocusField = $("*[data-autofocus='true']");
    if ($autofocusField.length > 0) {
      $autofocusField.focus();
      return $autofocusField.val($autofocusField.val());
    }
  });

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  $(function() {
    var $wrapper, base, sendRequest;
    if ($('#journal-issues').length === 0) {
      return;
    }
    $wrapper = $('#journal-issues');
    $('.link-to-issues').on('click', function(e) {
      var targetOffset;
      e.preventDefault();
      targetOffset = $('#issues-list-title').offset().top;
      return $('body, html').animate({
        scrollTop: targetOffset
      }, 300);
    });
    sendRequest = function(data) {
      return $.ajax({
        url: window.location.pathname,
        type: 'GET',
        data: data,
        dataType: 'json',
        beforeSend: function() {
          return $wrapper.addClass('disabled');
        },
        complete: function() {
          return $wrapper.removeClass('disabled');
        },
        success: function(ret) {
          var base;
          $wrapper.html(ret.html);
          return typeof (base = window.history).pushState === "function" ? base.pushState(ret.html, $('title')) : void 0;
        }
      });
    };
    $wrapper.on('change', '#year-select, #issue-select', function() {
      var $select, data;
      $select = $(this);
      data = {};
      data[$select.attr('name')] = $select.val();
      return sendRequest($.param(data));
    });
    window.onpopstate = (function(_this) {
      return function(e) {
        var scrollVal;
        scrollVal = window.scrollY;
        if (e.state && $wrapper.length > 0) {
          $wrapper.html(e.state);
        }
        return window.scroll(0, scrollVal);
      };
    })(this);
    if (window.history.state === null) {
      return typeof (base = window.history).replaceState === "function" ? base.replaceState($wrapper.html(), $('title')) : void 0;
    } else {
      return $wrapper.html(window.history.state);
    }
  });

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  $(function() {
    var $paginatorContainer, $sidebar, collapseSidebar, filter, paginator;
    if ($('#journals-filtered-list').length === 0) {
      return;
    }
    $paginatorContainer = $('#journals-filtered-list');
    paginator = new Paginator('/journal', $paginatorContainer);
    filter = new Filter($paginatorContainer, paginator);
    $sidebar = $('#sidebar');
    collapseSidebar = function(hideContent) {
      if (hideContent == null) {
        hideContent = true;
      }
      if (hideContent) {
        $sidebar.addClass('hidden');
      }
      return $(document).on('scroll', function(e) {
        if ($(this).scrollTop() < 180) {
          return $sidebar.fadeIn(300);
        } else {
          return $sidebar.fadeOut(300);
        }
      });
    };
    if ($sidebar.hasClass('hidden')) {
      return collapseSidebar(false);
    } else {
      return $paginatorContainer.one('load', function(e) {
        return collapseSidebar();
      });
    }
  });

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  var slice = [].slice;

  $(function() {
    var $wrapper, bindGaEvents, bindYaEvents;
    $wrapper = $('#wrapper');
    bindYaEvents = function() {
      var bindEvent, e, events, i, len, results;
      events = 1 <= arguments.length ? slice.call(arguments, 0) : [];
      if (typeof yaCounter15765706 === "undefined" || yaCounter15765706 === null) {
        return;
      }
      bindEvent = function(e) {
        var cond, eventKind, eventName, fn, selector;
        selector = e[0], eventKind = e[1], eventName = e[2], cond = e[3];
        fn = function() {
          if ((cond != null) && !cond()) {
            return;
          }
          yaCounter15765706.reachGoal(eventName);
          if ((typeof gon !== "undefined" && gon !== null) && gon.env !== 'production') {
            return console.log("Metrika triggered: " + eventName);
          }
        };
        return $wrapper.on(eventKind, selector, fn);
      };
      results = [];
      for (i = 0, len = events.length; i < len; i++) {
        e = events[i];
        results.push(bindEvent(e));
      }
      return results;
    };
    bindGaEvents = function() {
      var bindEvent, e, events, i, len, results;
      events = 1 <= arguments.length ? slice.call(arguments, 0) : [];
      if (typeof _gaq === "undefined" || _gaq === null) {
        return;
      }
      bindEvent = function(e) {
        var eventKind, eventName, eventValue, selector;
        selector = e[0], eventKind = e[1], eventName = e[2], eventValue = e[3];
        return $wrapper.on(eventKind, selector, function() {
          _gaq.push(['_trackEvent', eventName, eventValue]);
          if ((typeof gon !== "undefined" && gon !== null) && gon.env !== 'production') {
            return console.log("GA triggered: " + eventName + " " + eventValue);
          }
        });
      };
      results = [];
      for (i = 0, len = events.length; i < len; i++) {
        e = events[i];
        results.push(bindEvent(e));
      }
      return results;
    };

    bindYaEvents(
      ['.social-link', 'click', 'social_click'],
      ['.appstore-link', 'click', 'appstore_click'],
      ['.google-play-link', 'click', 'googleplay_click'],
      ['.search-hl', 'click', 'hl_click'],
      ['.ann-hl', 'click', 'ann_click'],
      ['.btn-comment', 'click', 'comment_click'],
      ['.quote-btn', 'click', 'blink_click'],
      ['.similar-link', 'click', 'similar_click'],
      ['.start-reading', 'click', 'cover_click'],
      ['.form-search', 'submit', 'search'],
      ['.fav-btn', 'click', 'fav_click'],
      ['.rate-article .star', 'click', 'rate_click', (function() {
        return gon.current_user_email; }) ],
      ['.pdf-btn', 'click', 'pdf_dl'],
      ['.obrazovach a', 'click', 'n1_click'],
      ['.obraz a', 'click', 'n1_click']
    );

    bindGaEvents(
      ['.start-reading', 'click', 'click', 'article_cover'],
      ['.pdf-btn', 'click', 'user', 'pdf_dl']
    );

    if ($.cookie('registration_completed')) {
      if ((typeof yaCounter15765706 !== "undefined" && yaCounter15765706 !== null) && _gaq) {
        yaCounter15765706.reachGoal('reg_complete');
        _gaq.push(['_trackEvent', 'user', 'reg_complete']);
      }
    }
    if ($.cookie('author_request_sent')) {
      if (typeof yaCounter15765706 !== "undefined" && yaCounter15765706 !== null) {
        yaCounter15765706.reachGoal('author_reg');
      }
    }
    if ($.cookie('registration_completed')) {
      $.cookie('registration_completed', '', {
        path: '/'
      });
    }
    if ($.cookie('author_request_sent')) {
      return $.cookie('author_request_sent', '', {
        path: '/'
      });
    }
  });

}).call(this);
// Generated by CoffeeScript 1.9.2
(function() {
  $(function() {
    if ($('.popup-holder').length === 0) {
      return;
    }
    window.popup = {};
    popup.setOffsets = function(elem) {
      var elemOffset;
      elemOffset = $(window).scrollTop() + (($(window).height() - elem.outerHeight()) / 2);
      return elem.css('top', elemOffset);
    };
    popup.showPopup = function(elem, speed) {
      var _holder, _popup;
      if (speed == null) {
        speed = 300;
      }
      _holder = $(".popup-" + elem);
      _popup = _holder.find('.popup-frame');
      _holder.css({
        'left': '0',
        'height': $('#wrapper').height()
      });
      _holder.fadeIn(speed);
      popup.setOffsets(_popup);
      $(window).on('scroll.pop', function() {
        return popup.setOffsets(_popup);
      });
      return $(window).on('resize.pop', function() {
        return popup.setOffsets(_popup);
      });
    };
    popup.closePopup = function(elem, speed) {
      var _holder;
      if (speed == null) {
        speed = 300;
      }
      _holder = elem.closest('.popup-holder');
      _holder.fadeOut(speed, function() {
        return _holder.css({
          "left": "-9999px"
        }).fadeOut(speed);
      });
      $(window).off('scroll.pop');
      return $(window).off('resize.pop');
    };
    $('.user-login-link').add('.user-registration-link').on('click', function(e) {
      e.preventDefault();
      return popup.showPopup($(this).data('target'));
    });
    $('#want-register').on('click', function(e) {
      e.preventDefault();
      popup.closePopup($(this), 0);
      return popup.showPopup($(this).data('target'), 0);
    });
    $('#want-restore').on('click', function(e) {
      e.preventDefault();
      popup.closePopup($(this), 0);
      return popup.showPopup($(this).data('target'), 0);
    });
    $('.popup-holder').on('click', '.close, .bg', function(e) {
      e.preventDefault();
      return popup.closePopup($(this));
    });
    $('.quote-btn').on('click', function(e) {
      e.preventDefault();
      return popup.showPopup($(this).data('target'));
    });
    $('.feedback-btn').on('click', function(e) {
      e.preventDefault();
      if (gon.current_user_email) {
        return popup.showPopup($(this).data('target'));
      }
    });
    if (navigator.userAgent.match(/iPad/i)) {
      $('.popup-info').show();
    }
    if (!gon.current_user_email) {
      return $('body').on('click', '.need-signin', function(e) {
        e.preventDefault();
        e.stopPropagation();
        return popup.showPopup('signin');
      });
    }
  });

}).call(this);
